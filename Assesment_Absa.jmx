          <stringProp name="BeanShellSampler.query">import org.apache.jmeter.util.JMeterUtils;
import com.zapper.reporting.ZapperReporting;
import java.util.Date;
import java.text.SimpleDateFormat;

String curr_dir = System.getProperty(&quot;user.dir&quot;);

String serverURL = null, db_ip = null,db_env = null;

String os = vars.get(&quot;os&quot;);
String config = &quot;&quot;;
long time = prev.getStartTime();
Date d = new Date(time);
SimpleDateFormat sdf = new SimpleDateFormat(&quot;yyyy-MM-dd(hh.mm)&quot;);
String timestamp = sdf.format(d);


if(os.toLowerCase().contains(&quot;windows&quot;))
{
	reportLocation = curr_dir+&quot;\\Results\\ZapperApps_&quot;+timestamp+&quot;.html&quot;;
	curr_dir = curr_dir+&quot;\\TestData\\&quot;;
	config = &quot;..\\lib\\extent-config.xml&quot;;
}
else
{
	reportLocation = curr_dir+&quot;/Results/Report_&quot;+timestamp+&quot;.html&quot;;
	curr_dir = curr_dir+&quot;/TestData/&quot;;	
	config = &quot;../lib/extent-config.xml&quot;;
}

vars.put(&quot;curr_dir&quot;,curr_dir);
env = env.toLowerCase();
ZapperReporting report = ZapperReporting.getInstance(reportLocation);
report.setConfig(config);
JMeterUtils.setProperty(&quot;Reporting&quot;,&quot;ready&quot;);



String[] x = null;
String new_dir= &quot;&quot;;
if(curr_dir.contains(&quot;\\&quot;))
{
	x = curr_dir.split(&quot;[\\\\]&quot;);

	for(int i =0; i&lt;x.length; i++)
	{
		 new_dir = new_dir + x[i] + &quot;//&quot;;
	}
}
else
{
	new_dir = curr_dir;
}

log.info(&quot;Server URL --&gt; &quot;+serverURL);
log.info(&quot;Server ENV --&gt; &quot;+env);
log.info(&quot;Server DB ENV --&gt; &quot;+db_env);
log.info(&quot;Curr dir --&gt;&quot; +curr_dir);
log.info(&quot;New dir --&gt;&quot; +new_dir);
log.info(&quot;New dir --&gt;&quot; +timestamp);


JMeterUtils.setProperty(&quot;ServerName&quot;,serverURL);
JMeterUtils.setProperty(&quot;env&quot;,env);
JMeterUtils.setProperty(&quot;db_env&quot;,db_env);
JMeterUtils.setProperty(&quot;serverkey_file&quot;,publickey);
JMeterUtils.setProperty(&quot;curr_dir&quot;,curr_dir);
JMeterUtils.setProperty(&quot;bean_dir&quot;, new_dir);
JMeterUtils.setProperty(&quot;Realtime&quot;,real);</stringProp>
